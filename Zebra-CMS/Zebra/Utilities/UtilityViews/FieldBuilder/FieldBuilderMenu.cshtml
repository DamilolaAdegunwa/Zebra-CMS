@inherits System.Web.Mvc.WebViewPage
@{
    Layout = null;
}

<input type="button" class="fieldbuilder-addfields-btn" value="Add Fields" />


<script>

    $(document).on('click', '.fieldbuilder-addfields-btn', function () {
        var query = "fullyqualifiedname=Zebra.Utilities.UtilityProcessor.FieldBuilderUtility,Zebra&method=RenderFieldBuilder";
        $.get("./RenderUtility?" + query, function (data) {
            $("body").append(data);
            $('body').append("<div class='black-overlay'></div>");
        });
    });

</script>
